{
    "manifest_version": 2,
    "name": "Codenames dark theme",
    "description": "Dark theme for the Codenames game",
    "version": "1.0",
    "browser_action": {
        "default_icon": "download.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["content.js"],
        "matches": ["https://www.horsepaste.com/*"]
    }]
}